{
  "singleQuote": false,
  "semi": true,
  "printWidth": 120,
  "tabWidth": 2,
  "trailingComma": "es5",
  "overrides": [
    {
      "files": "./src/{components,modules}/**/*.{ts,tsx}",
      "options": {
        "plugins": [
          "@trivago/prettier-plugin-sort-imports"
        ],
        "importOrder": [
          "^react",
          "^@?\\w",
          "<THIRD_PARTY_MODULES>",
          "^[./]"
        ]
      }
    }
  ]
}